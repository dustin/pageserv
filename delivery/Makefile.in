SHELL=/bin/sh
CC=@CC@
YACC=@YACC@
INCLUDES=-I@top_srcdir@/include
CFLAGS=$(INCLUDES) -DVERSION=\"$(VERSION)\" @CFLAGS@
AR=@AR@ cr
RANLIB=@RANLIB@

LIBS=@LIBS@

LDFLAGS=@LDFLAGS@ $(LIBS)

LDELIVER_OBJS=tap.o network.o modem.o serial.o
SOURCES=tap.c network.c network.c modem.c serial.c
EXES=taptest
STUFF=
JUNK=$(EXES) *.o *.a *.core core
AUTOJUNK=

all: deliver.a

taptest: taptest.o deliver.a
	$(CC) -o taptest taptest.o deliver.a @top_srcdir@/pageserv/utility.o

deliver.a: $(LDELIVER_OBJS)
	$(AR) $@ $(LDELIVER_OBJS)
	$(RANLIB) $@

depend:
	echo "" >> Makefile
	echo "# Automatically generated dependencies:" >> Makefile
	$(CC) $(CFLAGS) -M $(SOURCES) >> Makefile

# irix globbing shit.
irixhack.o:
	@touch irixhack.o

clean:  irixhack.o
	rm -f $(JUNK)

ultrabrite: irixhack.o
	rm -f $(JUNK) $(AUTOJUNK)
