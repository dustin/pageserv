MAJOR=2
MINOR=2
PATCH=0
VERSION=$(MAJOR).$(MINOR).$(PATCH)

SHELL=/bin/sh
CC=@CC@
YACC=@YACC@
CFLAGS=-g -DVERSION=\"$(VERSION)\"
AR=@AR@ cr
RANLIB=@RANLIB@

LIBS=@LIBS@

LDFLAGS=-g $(LIBS)

LDELIVER_OBJS=tap.o network.o checksum.o modem.o
SOURCES=tap.c network.c checksum.c network.c modem.c
EXES=taptest
STUFF=
JUNK=$(EXES) *.o *.a *.core core
AUTOJUNK=

all: $(EXES)

taptest: taptest.o deliver.a
	$(CC) -o taptest taptest.o deliver.a ../utility.o

deliver.a: $(LDELIVER_OBJS)
	$(AR) $@ $(LDELIVER_OBJS)
	$(RANLIB) $@

depend:
	echo "" >> Makefile
	echo "# Automatically generated dependencies:" >> Makefile
	$(CC) $(CFLAGS) -M $(SOURCES) >> Makefile

# irix globbing shit.
irixhack.o:
	@touch irixhack.o

clean:  irixhack.o
	rm -f $(JUNK)

ultrabrite: irixhack.o
	rm -f $(JUNK) $(AUTOJUNK)
